<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="pl.cctechnology.skicup2017" version="1.0.5" android-versionCode="10005">
  <name>Ski Cup 2017</name>
  <description>Konferencja technologiczna C&amp;C oraz Ski Cup 2017</description>
  <author href="http://cctechnology.pl" email="info@cctechnology.pl">CC Technology</author>
  <content src="index.html"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="android-minSdkVersion" value="15"/>
  <preference name="SplashScreen" value="screen" />
  <preference name="AutoHideSplashScreen" value="false" />
  <preference name="FadeSplashScreenDuration" value="900"/>
  <plugin name="cordova-plugin-console" source="npm" spec="~1.0.2"/>
  <plugin name="cordova-plugin-device" source="npm" spec="~1.1.1"/>
  <plugin name="cordova-plugin-geolocation" source="npm" spec="~2.1.0"/>
  <plugin name="cordova-plugin-inappbrowser" source="npm" spec="1.6.1"/>
  <plugin name="cordova-plugin-splashscreen" source="npm" spec="4.0.1"/>
